
<h1 class="indext_h1 mt-3">TO DO Lists</h1>
<span class="new_button">
    <%= link_to "New", new_list_path, class: "btn btn-light" %>
</span>

    <table class="table table-striped mt-3">

  <tr>
    <th>Title</th>
    <th>Note</th>
    <th>Actions</th>
  </tr>
  <% @lists.each do |list| %>
    <tr>
      <td><%= list.title%></td>
      <td><%= list.note %></td>
      <td><%= link_to "Edit", edit_list_path(list), class: "btn btn-light" %></td>
    </tr>
  <% end %>
</table>

